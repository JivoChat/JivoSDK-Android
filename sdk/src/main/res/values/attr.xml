<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="JivoChatButton">
        <attr name="clipToPadding" format="boolean" />
        <attr name="padding" format="dimension" />
        <attr name="backgroundTintList" format="reference" />
        <attr name="badgeColor" format="reference" />
    </declare-styleable>

    <declare-styleable name="JivoSDKToolbar">
        <attr name="logo" format="reference" />
        <attr name="hideLogo" format="boolean" />
        <attr name="title" format="string" />
        <attr name="subtitle" format="string" />
    </declare-styleable>

    <declare-styleable name="ExpandableTextView">
        <attr name="collapsedMaxLines" format="dimension" />
        <attr name="postfix" format="string" />
    </declare-styleable>

</resources>